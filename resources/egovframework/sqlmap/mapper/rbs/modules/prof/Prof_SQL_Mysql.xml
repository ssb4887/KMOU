<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
            "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="rbs.modules.prof.profMariaMapper">
	
	<select id="selectProfSn" parameterType="java.util.HashMap" resultType="dataMap">
		SELECT 				EMP_NO
							, EMP_NM
							, V_TEA_SN
		FROM 				empno_research
		WHERE 				EMP_NO = #{empNo}
	</select>
	
</mapper>            